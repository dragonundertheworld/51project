MACRO ASSEMBLER A51 V8.2.7.0
OBJECT MODULE PLACED IN .\Objects\MULTIPLECOMPUTER-COMMUNICATION-SLAVE.obj
ASSEMBLER INVOKED BY: D:\KEIL\C51\BIN\A51.EXE MULTIPLECOMPUTER-COMMUNICATION-SLAVE.a51 SET(SMALL) DEBUG PRINT(.\Listings
                      \MULTIPLECOMPUTER-COMMUNICATION-SLAVE.lst) OBJECT(.\Objects\MULTIPLECOMPUTER-COMMUNICATION-SLAVE.obj) EP

LOC  OBJ            LINE     SOURCE

                       1     ;从机初始化事口.
0000 7598F0            2     S_INIT: MOV SCON, #0F0H
                       3     ;11110000->SCON
                       4     ;使能事口收发，并设置工作在模式三
0003 758920            5     MOV TMOD, #20H
                       6     ;设置定时器I1为工作模式2
0006 758DFD            7     MOV TH1,#0FDH
                       8     ;波特率设置为9600
0009 758BFD            9     MOV TL1,#0FDH
000C D28E             10     SETB TR1
                      11     ;开启定时器1
                      12     ;从机检测是否收到主机请求
000E                  13     CHECK:
000E 3098FD           14     JNB RI, $
                      15     ;若RI等于0，则继续等待
0011 C298             16     CLR RI
                      17     ;清除接收中断标志RI
0013 E599             18     MOV A,SBUF
                      19     ;接受主机发来的地址信息
0015 B401F6           20     CJNE A, #1,CHECK ;判断发送的地址是否 与本机地址相等，address(这里是1)
                             示本机地址
                      21     ;若不相等则继续等待，不做处理
0018 C29D             22     CLR SM2
                      23     ;将SM2位清零
                      24     ;从机应答
001A                  25     ACK:
001A 3099FD           26     JNB TI, $;trasmission
                      27     ;判断发送缓冲区是否为空?
001D C299             28     CLR TI
                      29     ;清零发送中断标志II
001F 759901           30     MOV SBUF,#1;向主机发送应答信号，即发回本机地址(1)给主机
                      31     ;从机接收主机数据(采用奇校验方式)
0022                  32     RECEIVE:
0022 3098FD           33     JNB RI,$
0025 C298             34     CLR RI
0027 E599             35     MOV A, SBUF
0029 A2D0             36     MOV C,P;将奇偶标志位传给C
002B 5003             37     JNC ERROR;若c=0，则传输出错，转至出错处理
002D C2E7             38     CLR ACC.7
                      39     ;清除A中最高位做奇偶校验位
002F 22               40     RET
0030 0130             41     ERROR: AJMP ERROR
                      42     ;若不相等则继续等待，不做处理
0032 C29D             43     CLR SM2
                      44     ;将SM2位清零
0034 F5A0             45     MOV P2,A
                      46     END
